<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="sg.edu.nus.learnandroid">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".CourseMapActivity" />
        <activity android:name=".UserAccountActivity" />
        <activity android:name=".NeedHelpActivity" />
        <activity android:name=".ForumActivity" />
        <activity android:name=".ProfileEditActivity" />
        <activity android:name=".ChangePasswordActivity" />
        <activity android:name=".CourseFragmentActivity"></activity>
    </application>

</manifest>